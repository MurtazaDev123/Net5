        function GetSelected(p, industry_array) {

            var returnValue = false;

            for (var i = 0; i < industry_array.length; i++) {
                //console.log(industry_array[i]["Id"]);
                if (industry_array[i]["ContentTypeUploadId"] == p) {
                    returnValue = true;
                    break;
                }
            }

            return returnValue;
        }

        function cityLoad(id) {

            var control = $("#ddlcity");
            control.select2();

            control.attr('disabled', 'disabled');
            control.prepend($('<option></option>').html('Loading...'));

            $.ajax({
                type: "POST",
                url: '/Admin/CitiesByState',
                data: { parent_id: $("#ddlstate").val() },
                success: function (data) {

                    control.empty();
                    control.append($('<option>').text("Select").attr('value', "").attr('selected', 'selected'));

                    $.each(data.data, function (index, value) {
                        if (value["Active"] == true) {
                            control.append($('<option>').text(value["Name"]).attr('value', value["Id"]));
                        }
                    });

                    if (id != 0) {
                        control.val(id);
                    }

                },
                failure: function (data) {
                },
                complete: function () {
                    control.removeAttr('disabled');
                }
            });
        }

