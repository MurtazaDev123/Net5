
@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section HeadSection {
    <style>
    </style>

}

<div class="content-wrapper">
    <div class="page-header">
        <h3 class="page-title">
            Partners
        </h3>
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Partners</a></li>
                <li class="breadcrumb-item active" aria-current="page">Partners Setup</li>
            </ol>
        </nav>
    </div>

    <div class="card">
        <div class="card-body">

            <div class="row">
                <div class="col-sm-12">
                    <button id="add_popup" class="btn btn-primary btn-sm" style="width: 100px;"><i class="fa fa-plus"></i> Add New</button>
                    <button id="edit_popup" class="btn btn-info btn-sm" style="width: 100px;" disabled><i class="fa fa-edit"></i> Edit</button>
                    <button id="btn_delete" class="btn btn-danger btn-sm" style="width: 100px;" disabled><i class="fa fa-trash"></i> Delete</button>
                    <button id="btn_restore" class="btn btn-info btn-sm" style="width: 100px; display: none;" disabled><i class="fa fa fa-window-restore"></i> Restore</button>
                    <a href="/admin/dashboard" class="btn btn-secondary btn-sm" style="width: 100px;"><i class="fa fa-arrow-circle-left"></i> Back</a>

                    <div class="float-right">
                        <a class="btn btn-success btn-sm" id="btn_current" style="width: 100px;" href="#">Current</a>
                        <a class="btn btn-outline-danger btn-sm" id="btn_archive" style="width: 100px;" href="#">Archive</a>
                    </div>
                </div>
            </div>

            <br />

            <div class="row">
                <div class="col-12">
                    <div class="table-responsive table-current-listing">
                        <table id="current-listing" class="table table-striped table-hover table-bordered" style="width: 100%;">
                            <thead>
                                <tr>
                                    <th>Id</th>
                                    <th>Partner Name</th>
                                    <th>Type</th>
                                    <th>Category</th>
                                    <th>Country</th>
                                    <th>Email</th>
                                    <th>Mobile #</th>
                                    <th>Created On</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                        </table>
                    </div>

                    <div class="table-responsive table-archive-listing" style="display: none;">
                        <table id="archive-listing" class="table table-striped table-hover table-bordered" style="width: 100%;">
                            <thead>
                                <tr>
                                    <th>Id</th>
                                    <th>Partner Name</th>
                                    <th>Type</th>
                                    <th>Category</th>
                                    <th>Country</th>
                                    <th>Email</th>
                                    <th>Mobile #</th>
                                    <th>Created On</th>
                                    <th>Status</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modal_setup" tabindex="-1" role="dialog" aria-labelledby="modal_setup" data-backdrop="static" data-keyboard="false" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modal_title">Add/Edit Partners</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <form class="form-sample" method="post" id="entry-form" action="/Admin/Users">
                    <div class="alert alert-danger error_msg" style="display: none;">This is error</div>
                    <div class="row">
                        <div class="col-sm-9">

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtid">ID</label>
                                <div class="col-sm-4">
                                    <input type="text" class="form-control" id="txtid" name="txtid" readonly />
                                </div>
                                <label class="col-sm-5"><small>* Auto Generated</small></label>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtname">Full Name</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txtname" name="txtname" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddltype">Type</label>
                                <div class="col-sm-9">
                                    <select id="ddltype" name="ddltype" class="form-control"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlcategory">Category</label>
                                <div class="col-sm-9">
                                    <select id="ddlcategory" name="ddlcategory" class="form-control"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtcontactperson">Contact Person</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txtcontactperson" name="txtcontactperson" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txttelephone">Telephone</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txttelephone" name="txttelephone" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txttype">Mobile No.</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txtmobileno" name="txtmobileno" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtuserid">Email Address</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txtuserid" name="txtuserid" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row" id="divPassChange" style="display:none;">
                                <label class="col-sm-3 col-form-label">Password Change</label>
                                <div class="col-sm-9">
                                    <div class="form-check form-check-flat form-check-primary">
                                        <label class="form-check-label">
                                            <input type="checkbox" id="chkPassword" name="chkPassword" class="form-check-input">
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtpassword">Password <small style="color: red;">*</small></label>
                                <div class="col-sm-9">
                                    <input type="password" class="form-control" id="txtpassword" name="txtpassword" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlcountry">Country</label>
                                <div class="col-sm-9">
                                    <select id="ddlcountry" name="ddlcountry" class="form-control"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlstate">State</label>
                                <div class="col-sm-9">
                                    <select id="ddlstate" name="ddlstate" class="form-control"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlcity">City</label>
                                <div class="col-sm-9">
                                    <select id="ddlcity" name="ddlcity" class="form-control"><option value="0">Select</option></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtemailaddress">Address</label>
                                <div class="col-sm-9">
                                    <textarea rows="2" class="form-control" name="txtaddress" id="txtaddress"></textarea>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlcontentype">Type of Content Produced</label>
                                <div class="col-sm-9">
                                    <select id="ddlcontentype" name="ddlcontentype" class="form-control"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="ddlcontentypeupload">Type of Content you plan to upload</label>
                                <div class="col-sm-9">
                                    <select id="ddlcontentypeupload" name="ddlcontentypeupload" class="multiple-select2 form-control" multiple="multiple"></select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label" for="txtmont">Monetization</label>
                                <div class="col-sm-9">
                                    <input type="number" class="form-control" id="txtmont" name="txtmont" autocomplete="off" />
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label">Status</label>
                                <div class="col-sm-9">
                                    <div class="form-check form-check-flat form-check-primary">
                                        <label class="form-check-label">
                                            <input type="checkbox" id="chkActive" name="chkActive" class="form-check-input" checked="checked">
                                            <i class="input-helper"></i>
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label">Approval</label>
                                <div class="col-sm-9">
                                    <div class="form-check form-check-flat form-check-primary">
                                        <label class="form-check-label">
                                            <input type="checkbox" id="chkApproval" name="chkApproval" class="form-check-input"> You mark this checkbox then partner uploaded video cannot be show to website without admin approval.
                                            <i class="input-helper"></i>
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="col-sm-3 text-center">
                            <form id="form1" method="post" action="/Inventory/ImportItems">
                                <img id="profile_picture_img" src="/content/images/placeholder/product.jpg" style="width: 100%; height: auto;" alt="user image">
                                <a id="profile_picture" href="#">Select Image</a>
                                <input name="image" type="file" style="display: none;" id="hidden_file" />
                                <input type="hidden" id="hidden_image_name" name="hidden_image_name" value="" />
                            </form>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer">
                <button type="button" id="btnSaveRecord" class="btn btn-success">Save Record</button>
                <button type="button" class="btn btn-light" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>


@section FooterSection {
    <script>

        var current_id = 0;

        var currentListURL = '/Admin/CurrentPartners';
        var archiveListURL = '/Admin/ArchivePartners';
        var searchByIdURL = '/Admin/PartnerById';
        var moveToArchiveURL = '/Admin/MoveToArchiveUser';
        var redirectionURL = '/admin/partners';
        var saveURL = '/Admin/SaveUser';

        var current_table = bindGrid($('#current-listing'), currentListURL);
        var archive_table = bindGrid($('#archive-listing'), archiveListURL);

        fillDropDown("/Admin/CurrentPartnerType", $("#ddltype"), undefined, undefined, undefined, undefined, undefined, undefined, true, $("#modal_setup"));
        fillDropDown("/Admin/CurrentPartnerCategory", $("#ddlcategory"), undefined, undefined, undefined, undefined, undefined, undefined, true, $("#modal_setup"));

        fillDropDown("/Admin/CurrentCountries", $("#ddlcountry"), undefined, undefined, "/Admin/StatesByCountry/", $("#ddlstate"), undefined, undefined, true, $("#modal_setup"));

        fillDropDown("/Admin/CurrentPartnerContentype", $("#ddlcontentype"), undefined, undefined, undefined, undefined, undefined, undefined, true, $("#modal_setup"));
        //fillDropDown("/Admin/CurrentPartnerContentypeUpload", $("#ddlcontentypeupload"), undefined, undefined, undefined, undefined, undefined, undefined, true, $("#modal_setup"));

        $("#ddlcity").select2();
        $("#ddlcontentypeupload").select2();

        LoadContentTypeUpload('');

        $('#current-listing tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');

                $("#edit_popup").attr('disabled', 'disabled');
                $("#btn_delete").attr('disabled', 'disabled');
            }
            else {
                current_table.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');

                $("#edit_popup").removeAttr('disabled');
                $("#btn_delete").removeAttr('disabled');
            }
        });

        $('#archive-listing tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');

                $("#btn_restore").attr('disabled', 'disabled');
            }
            else {
                archive_table.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');

                $("#btn_restore").removeAttr('disabled');
            }
        });

        $("#add_popup").on("click", function (e) {
            $('#divPassChange').hide();
            $('#txtpassword').attr("disabled", false);
            current_id = 0;
            e.preventDefault();
            resetPopup();
            $("#modal_setup").modal('show');
        });

        $("#edit_popup").on("click", function (e) {
            e.preventDefault();

            var id = current_table.$('tr.selected')[0].id;
            current_id = current_table.$('tr.selected')[0].id;

            resetPopup();

            $("#modal_setup").modal('show');

            $.ajax({
                type: "POST",
                url: searchByIdURL,
                data: { 'id': id },
                success: function (data) {

                    $("#txtid").val(data["Id"]);
                    $("#txtname").val(data["FullName"]);
                    $("#ddltype").val(data["PartnerType"]["Id"]).trigger('change');
                    $("#ddlcategory").val(data["PartnerCategory"]["Id"]).trigger('change');

                    $("#txtcontactperson").val(data["ContactPerson"]);
                    $("#txttelephone").val(data["Telephone"]);
                    $("#txtmobileno").val(data["MobileNo"]);
                    $("#txtuserid").val(data["EmailAddress"]);

                    $("#ddlcountry").val(data["Country"]["Id"]).trigger('change');
                    $("#ddlstate").val(data["State"]["Id"]).trigger('change');
                    //$("#ddlcity").val(data["City"]["Id"]).trigger('change');

                    cityLoad(data["City"]["Id"]);

                    $("#txtaddress").val(data["Address"]);
                    $("#ddlcontentype").val(data["PartnerContentType"]["Id"]).trigger('change');
                    $("#ddlcontentypeupload").val(data["PartnerContentTypeUpload"]["ContentTypeUploadId"]).trigger('change');

                    LoadContentTypeUpload(data["PartnerContentTypeUpload"])
                    
                    $("#chkActive").prop('checked', data["Active"]);
                    $("#chkApproval").prop('checked', data["Approval"]);
                    $('#divPassChange').show();
                    $('#txtpassword').attr("disabled", true);

                    $("#txtmont").val(data["Monetization"]);

                    $("#hidden_image_name").val(data["ProfilePicture"]);

                    $("#profile_picture_img").attr('src', '/content/uploads/users/' + data['ProfilePicture']);
                },
                failure: function (data) {
                    alert(data);
                },
                always: function () {
                    console.log('i am in always');
                }
            });
        });

        $("#btn_delete").on("click", function (e) {
            e.preventDefault();
            var id = current_table.$('tr.selected')[0].id;
            moveRecord(id, 2, moveToArchiveURL, redirectionURL);
        });

        $("#btn_restore").on("click", function (e) {
            e.preventDefault();
            var id = archive_table.$('tr.selected')[0].id;
            moveRecord(id, 1, moveToArchiveURL, redirectionURL);
        });

        $("#chkPassword").change(function () {
            if (this.checked) {
                $('#txtpassword').attr("placeholder", "Type new password here");
                $('#txtpassword').attr("disabled", false);
            } else {
                $('#txtpassword').attr("placeholder", "");
                $('#txtpassword').attr("disabled", true);
            }
        });

        $("#ddlstate").on('change', function (e) {
            e.preventDefault();
            cityLoad(0);
        });

        $("#btnSaveRecord").on("click", function (e) {

            e.preventDefault();

            var hasFocus = false;
            $("#error_msg").hide();

            // validation
            if ($("#txtname").val().trim() == "") {
                $("#txtname").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#txtname").parent().parent().addClass('has-danger');

                $("#txtname").focus();
                hasFocus = true;
            } else {
                $("#txtname").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#txtname").parent().parent().removeClass('has-danger');
            }

            if ($("#txtuserid").val().trim() == "") {
                $("#txtuserid").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#txtuserid").parent().parent().addClass('has-danger');

                $("#txtuserid").focus();
                hasFocus = true;
            } else {
                $("#txtuserid").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#txtuserid").parent().parent().removeClass('has-danger');
            }

            if ($("#ddltype").val() == "0") {
                $("#ddltype").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddltype").parent().parent().addClass('has-danger');

                $("#ddltype").focus();
                hasFocus = true;
            } else {
                $("#ddltype").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddltype").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlcategory").val() == "0") {
                $("#ddlcategory").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlcategory").parent().parent().addClass('has-danger');

                $("#ddlcategory").focus();
                hasFocus = true;
            } else {
                $("#ddlcategory").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlcategory").parent().parent().removeClass('has-danger');
            }

            if ($("#txtcontactperson").val().trim() == "") {
                $("#txtcontactperson").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#txtcontactperson").parent().parent().addClass('has-danger');

                $("#txtcontactperson").focus();
                hasFocus = true;
            } else {
                $("#txtcontactperson").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#txtcontactperson").parent().parent().removeClass('has-danger');
            }

            if ($("#txtuserid").val().trim() == "") {
                $("#txtuserid").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#txtuserid").parent().parent().addClass('has-danger');

                $("#txtuserid").focus();
                hasFocus = true;
            } else {
                $("#txtuserid").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#txtuserid").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlcountry").val() == "0") {
                $("#ddlcountry").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlcountry").parent().parent().addClass('has-danger');

                $("#ddlcountry").focus();
                hasFocus = true;
            } else {
                $("#ddlcountry").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlcountry").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlstate").val() == "0") {
                $("#ddlstate").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlstate").parent().parent().addClass('has-danger');

                $("#ddlstate").focus();
                hasFocus = true;
            } else {
                $("#ddlstate").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlstate").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlcity").val() == "0") {
                $("#ddlcity").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlcity").parent().parent().addClass('has-danger');

                $("#ddlcity").focus();
                hasFocus = true;
            } else {
                $("#ddlcity").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlcity").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlcontentype").val() == "0") {
                $("#ddlcontentype").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlcontentype").parent().parent().addClass('has-danger');

                $("#ddlcontentype").focus();
                hasFocus = true;
            } else {
                $("#ddlcontentype").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlcontentype").parent().parent().removeClass('has-danger');
            }

            if ($("#ddlcontentypeupload").val() == "0") {
                $("#ddlcontentypeupload").parent().parent().find('label').addClass('mt-2 text-danger');
                $("#ddlcontentypeupload").parent().parent().addClass('has-danger');

                $("#ddlcontentypeupload").focus();
                hasFocus = true;
            } else {
                $("#ddlcontentypeupload").parent().parent().find('label').removeClass('mt-2 text-danger');
                $("#ddlcontentypeupload").parent().parent().removeClass('has-danger');
            }


            if (current_id == 0) {

                if ($("#txtpassword").val().trim() == "") {
                    $("#txtpassword").parent().parent().find('label').addClass('mt-2 text-danger');
                    $("#txtpassword").parent().parent().addClass('has-danger');

                    $("#txtpassword").focus();
                    hasFocus = true;
                } else {
                    $("#txtpassword").parent().parent().find('label').removeClass('mt-2 text-danger');
                    $("#txtpassword").parent().parent().removeClass('has-danger');
                }

            }

            if (hasFocus) {
                return false;
            }

            //alert($("#hidden_image_name").val());
            //return false;

            console.log("test");

            $.ajax({
                type: "POST",
                url: saveURL,
                data: {
                    'Id': $("#txtid").val(),
                    'UserName': $("#txtname").val(),
                    'Type': $("#ddltype").val(),
                    'Category': $("#ddlcategory").val(),
                    'ContactPerson': $("#txtcontactperson").val(),
                    'Telephone': $("#txttelephone").val(),
                    'MobileNo': $("#txtmobileno").val(),
                    'UserId': $("#txtuserid").val(),
                    'Password': $("#txtpassword").val(),
                    'Country': $("#ddlcountry").val(),
                    'State': $("#ddlstate").val(),
                    'City': $("#ddlcity").val(),
                    'Address': $("#txtaddress").val(),
                    'TypeContent': $("#ddlcontentype").val(),
                    'TypeContentUpload': $("#ddlcontentypeupload").val(),
                    'Monetization': $("#txtmont").val(),
                    'Active': $("#chkActive").is(':checked'),
                    'Approval': $("#chkApproval").is(':checked'),
                    'IsPassChange': $("#chkPassword").is(':checked'),
                    'image_name': $("#hidden_image_name").val()
                },
                traditional: true,
                success: function (data) {

                    console.log(data);

                    if (data["ErrorCode"] == "000") {
                        if (data["EntryLevel"] == "1") {
                            SwalSuccesswithRedirect('Success!', 'Record save successfully.', redirectionURL, $("#modal_setup"));
                        } else {
                            var row_index = current_table.$('tr.selected')[0]._DT_RowIndex;
                            var data = current_table.row(row_index).data();

                            data["FullName"] = $("#txtname").val();
                            data["PartnerType"]["Id"] = $("#ddltype").val();
                            data["PartnerType"]["Title"] = $("#ddltype option:selected").text();
                            data["PartnerCategory"]["Id"] = $("#ddlcategory").val();
                            data["PartnerCategory"]["Title"] = $("#ddlcategory option:selected").text();
                            data["Country"]["Id"] = $("#ddlcountry").val();
                            data["Country"]["Title"] = $("#ddlcountry option:selected").text();
                            data["EmailAddress"] = $("#txtuserid").val();
                            data["MobileNo"] = $("#txtmobileno").val();
                            data["FullName"] = $("#txtname").val();
                            data["Active"] = $("#chkActive").is(':checked');
                            
                            current_table.row(row_index).data(data);

                            SwalSuccesswithRedirect('Success!', 'Record edit successfully.', undefined, $("#modal_setup"));
                        }
                    }
                    else {
                        //alert('failed to update record');
                        $("#error_msg").text('failed to update record').show();
                    }
                },
                failure: function (data) {
                    alert(data);
                },
                always: function () {
                    //console.log('i am in always');
                }
            });
        });

        $("#profile_picture").on('click', function () {
            $("#hidden_file").click();
            return false;
        });

        $("#hidden_file").on('change', function (e) {

            e.preventDefault();

            $.ajax({
                url: "/Admin/UploadUserImage",
                type: "POST",
                data: new FormData(this.form),
                contentType: false,
                cache: false,
                processData: false,
                beforeSend: function () {
                    //$("#preview").fadeOut();
                    //$("#err").fadeOut();
                },
                success: function (data) {
                    console.log(data);
                    if (data['ErrorCode'] == '000') {

                        $("#profile_picture_img").attr('src', '/content/uploads/users/' + data['FileName']);
                        $("#hidden_image_name").val(data['FileName']);
                    }
                    else if (data['ErrorCode'] == '001') {
                        alert('file not found');
                    } else if (data['ErrorCode'] == '002') {
                        alert('invalid file extension');
                    } else {
                        alert('unhandled exception');
                    }
                },
                error: function (e) {
                    console.log(e);
                    alert(e);
                }
            });
        });

